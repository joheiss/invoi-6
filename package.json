    "@ngrx/router-store": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@ngrx/store-devtools": "^5.2.0",
    "angularfire2": "^5.0.0-rc.6",
    "body-parser": "^1.18.2",
    "command-line-args": "^4.0.7",
    "core-js": "^2.4.1",
    "express-jwt": "^5.3.0",
