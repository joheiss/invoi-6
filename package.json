{
  "name": "invoicing",
  "version": "1.0.0",
  "author": "Josef Heiss, JOVISCO GmbH",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:wossl": "ng serve --locale de-DE --proxy-config ./proxy.conf.json",
    "start": "ng serve --locale de-DE --proxy-config ./proxy.conf.json --ssl 1 --ssl-key localhost.key --ssl-cert localhost.pem",
    "start:prod": "ng serve --prod --locale de-DE --proxy-config ./proxy.conf.json --ssl 1 --ssl-key localhost.key --ssl-cert localhost.pem",
    "start:prod-wossl": "ng serve --prod",
    "start:ssl": "ng serve --locale de-DE --proxy-config ./proxy.conf.json --ssl 1 --ssl-key key.pem --ssl-cert cert.pem",
    "old-start-server": "./node_modules/.bin/ts-node --icu-data-dir=node_modules/full-icu ./server/server.ts --secure",
    "start-server": "./node_modules/.bin/ts-node server/server.ts --secure",
    "server": "./node_modules/.bin/nodemon  -w ./server --ext \".ts\" --exec \"npm run start-server\"",
    "build": "ng build --configuration=production",
    "test": "ng test",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e:protracor": "ng e2e",
    "e2e": "cypress open",
    "env": "SET NODE_ICU_DATA=./node_modules/full-icu",
    "httpserver": "http-server -c-1 dist/invoi-6",
    "firebase:deploy": "firebase deploy",
    "firebase:deploy-dev": "firebase deploy -f jovisco-invoicing"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "transformIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/fire": "^5.1.1",
    "@angular/flex-layout": "^7.0.0-beta.22",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/material": "^7.2.0",
    "@angular/material-moment-adapter": "^7.2.0",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/platform-server": "^7.1.4",
    "@angular/pwa": "^0.11.4",
    "@angular/router": "^7.1.4",
    "@angular/service-worker": "^7.1.4",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/entity": "^7.0.0",
    "@ngrx/router-store": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "@ngrx/store-devtools": "^7.0.0",
    "core-js": "^2.5.7",
    "firebase": "^5.7.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "typescript": "^3.1.6",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.1.2",
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular/cli": "7.1.4",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/language-service": "^7.1.4",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "codelyzer": "^4.5.0",
    "cypress": "^3.1.4",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "jasmine-core": "^3.3.0",
    "jasmine-marbles": "^0.4.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.2",
    "jest-sonar-reporter": "^2.0.0",
    "ngrx-store-freeze": "^0.2.4",
    "nodemon": "^1.18.9",
    "protractor": "^5.4.1",
    "rxjs-tslint": "^0.1.6",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0"
  }
}
