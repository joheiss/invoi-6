{
  "name": "invoicing",
  "version": "0.0.1",
  "author": "Josef Heiss, JOVISCO GmbH",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:wossl": "ng serve --locale de-DE --proxy-config ./proxy.conf.json",
    "start": "ng serve --locale de-DE --proxy-config ./proxy.conf.json --ssl 1 --ssl-key localhost.key --ssl-cert localhost.pem",
    "start:prod": "ng serve --prod --locale de-DE --proxy-config ./proxy.conf.json --ssl 1 --ssl-key localhost.key --ssl-cert localhost.pem",
    "start:ssl": "ng serve --locale de-DE --proxy-config ./proxy.conf.json --ssl 1 --ssl-key key.pem --ssl-cert cert.pem",
    "old-start-server": "./node_modules/.bin/ts-node --icu-data-dir=node_modules/full-icu ./server/server.ts --secure",
    "start-server": "./node_modules/.bin/ts-node server/server.ts --secure",
    "server": "./node_modules/.bin/nodemon  -w ./server --ext \".ts\" --exec \"npm run start-server\"",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "env": "SET NODE_ICU_DATA=./node_modules/full-icu"
  },
  "private": true,
