{
  "name": "invoicing",
  "version": "1.0.0",
  "author": "Josef Heiss, JOVISCO GmbH",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:wossl": "ng serve --locale de-DE --proxy-config ./proxy.conf.json",
    "start": "ng serve --locale de-DE --proxy-config ./proxy.conf.json --ssl 1 --ssl-key localhost.key --ssl-cert localhost.pem",
    "start:prod": "ng serve --prod --locale de-DE --proxy-config ./proxy.conf.json --ssl 1 --ssl-key localhost.key --ssl-cert localhost.pem",
    "start:prod-wossl": "ng serve --prod",
    "start:ssl": "ng serve --locale de-DE --proxy-config ./proxy.conf.json --ssl 1 --ssl-key key.pem --ssl-cert cert.pem",
    "old-start-server": "./node_modules/.bin/ts-node --icu-data-dir=node_modules/full-icu ./server/server.ts --secure",
    "start-server": "./node_modules/.bin/ts-node server/server.ts --secure",
    "server": "./node_modules/.bin/nodemon  -w ./server --ext \".ts\" --exec \"npm run start-server\"",
    "build": "ng build --configuration=production",
    "test": "ng test",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e:protracor": "ng e2e",
    "e2e": "cypress open",
    "env": "SET NODE_ICU_DATA=./node_modules/full-icu",
    "httpserver": "http-server -c-1 dist/invoi-6",
    "firebase:deploy": "firebase deploy",
    "firebase:deploy-dev": "firebase deploy -f jovisco-invoicing"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "transformIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.3",
    "@angular/cdk": "~11.0.1",
    "@angular/common": "~11.0.3",
    "@angular/compiler": "~11.0.3",
    "@angular/core": "~11.0.3",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.0.3",
    "@angular/localize": "^11.0.3",
    "@angular/material": "^11.0.1",
    "@angular/material-moment-adapter": "^11.0.1",
    "@angular/platform-browser": "~11.0.3",
    "@angular/platform-browser-dynamic": "~11.0.3",
    "@angular/platform-server": "~11.0.3",
    "@angular/pwa": "^0.1001.1",
    "@angular/router": "~11.0.3",
    "@angular/service-worker": "^11.0.3",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@ngrx/effects": "^10.0.0",
    "@ngrx/entity": "^10.0.0",
    "@ngrx/router-store": "^10.0.0",
    "@ngrx/store": "^10.0.0",
    "@ngrx/store-devtools": "^10.0.0",
    "core-js": "^3.6.5",
    "firebase": "^8.1.2",
    "hammerjs": "^2.0.8",
    "jovisco-domain": "^1.0.69",
    "lodash": ">=4.17.20",
    "luxon": "^1.25.0",
    "moment": "^2.27.0",
    "rxjs": "~6.6.3",
    "rxjs-compat": "^6.6.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1100.3",
    "@angular/cli": "~11.0.3",
    "@angular/compiler-cli": "~11.0.3",
    "@angular/language-service": "~11.0.3",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/luxon": "^1.24.4",
    "@types/node": "^14.6.4",
    "codelyzer": "^6.0.0",
    "cypress": "^5.1.0",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "jasmine-core": "^3.5.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "jest": "^26.4.2",
    "jest-preset-angular": "^8.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "ngrx-store-freeze": "^0.2.4",
    "nodemon": "^2.0.4",
    "protractor": "~7.0.0",
    "rxjs-tslint": "^0.1.8",
    "tar": ">=4.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
